<!DOCTYPE beans PUBLIC  "-//SPRING//DTD BEAN//EN"
        "http://www.springframework.org/dtd/http://www.springframework.org/dtd/spring-beans.dtd">

<beans>


    <bean id="wca1" class="org.summerframework.aop.attributes.WildcardAttributeRegistry">
        <property name="map">
            <map>
                <entry key="setAge">
                    <value>setAgeAtt</value>
                </entry>
                <entry key="getAge">
                    <list>
                        <value>zero</value>
                        <ref bean="rod"/>
                    </list>
                </entry>
            </map>
        </property>
    </bean>

    <bean id="rod" class="org.summerframework.beans.TestBean">
        <property name="name">
            <value>Rod</value>
        </property>
        <property name="age">
            <value>33</value>
        </property>
    </bean>

</beans>		
	
	